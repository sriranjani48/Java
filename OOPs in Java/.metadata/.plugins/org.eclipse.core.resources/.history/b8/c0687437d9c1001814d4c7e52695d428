
public class PrimeNumbers {
	
	public static int isprime(int num) {
		int 
		if(num <= 1) {
			return num;
		}
		else {
			for(int i = 2; i <= num; i++) {
				for(int j = 2; j < i; j++) {
					if(i % j == 0) {
						break;
					}
					else if(i == j+1) {
						return i;
					}
				}
				
			}
			
		}
	}
	
	public static void main(String[] args) {
		

	}

}
