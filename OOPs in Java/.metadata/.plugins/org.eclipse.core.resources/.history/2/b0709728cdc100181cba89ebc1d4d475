
public class EvenOdd {
	
	public void evenodd(int num) {
		if(num == 0) {
			System.out.println('0');
		}
		else if(num % 2 == 0){
			System.out.println("Even");
		}
		else if(num % 2 != 0){
			System.out.println("Odd");
		}
		else {
			return;
		}
		//return null;
	}
	
	public static void main(String args[]) {
		
		EvenOdd ob = new EvenOdd();
		
		int num1 = 10;
		ob.evenodd(num1);
		
		/*int num2 = 1111;
		evenodd(num2);
		
		int num3 = 0;
		evenodd(num3);
		
		int num4 = -10;
		evenodd(num4);
		
		int num5 = ' ';
		evenodd(num5);*/
	}
}
