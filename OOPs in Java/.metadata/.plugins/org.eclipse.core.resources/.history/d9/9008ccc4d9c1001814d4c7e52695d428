
public class PrimeNumbers {
	
	public static int prime(int num) { 
		if(num <= 1) {
			return num;
		}
		else {
			for(int i = 2; i <= num; i++) {
				for(int j = 2; j < i; j++) {
					if(i % j == 0) {
						break;
					}
					else if(i == j+1) {
						System.out.println(i + " ");
					}
				}
				
			}
			
		}
		return 0;
	}
	
	public static void main(String[] args) {
		prime(10);

	}

}
